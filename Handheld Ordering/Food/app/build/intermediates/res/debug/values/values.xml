    </style>
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
    <style name="alter_order_number_layout" parent="fill_wrap">
        <item name="android:layout_height">60dp</item>
        <item name="android:orientation">horizontal</item>
    </style>
    <style name="alter_order_number_text">
        <item name="android:layout_width">60dp</item>
        <item name="android:layout_height">60dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">20dp</item>
        <item name="android:background">@drawable/btn_keypad_grey</item>
    </style>
    <style name="fill">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
    </style>
    <style name="fill_wrap">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="input_box" parent="fill_wrap">
        <item name="android:layout_height">40dp</item>
        <item name="android:singleLine">true</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:textSize">20dp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:background">@drawable/shape_input_box</item>
    </style>
    <style name="login_button">
