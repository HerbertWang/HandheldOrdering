<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <RelativeLayout
        android:id="@+id/relativeLayoutLevelThree"
        style="@style/fill_wrap"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp" >

        <LinearLayout
            style="@style/wrap"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:paddingTop="5dp" >

            <TextView
                android:id="@+id/tvLevelThreeStatus"
                android:layout_width="18dp"
                android:layout_height="18dp" />

            <TextView
                android:id="@+id/tvLevelThreeName"
                style="@style/wrap"
                android:layout_marginLeft="2dp"
                android:textColor="@color/black"
                android:textSize="18dp" />
        </LinearLayout>

        <TextView
            android:id="@+id/tvLevelThreePrice"
            style="@style/wrap"
            android:layout_alignParentRight="true"
            android:layout_marginTop="5dp"
            android:text="+1.00"
            android:textColor="@color/black"
            android:textSize="18dp" />
    </RelativeLayout>

    <LinearLayout
        android:id="@+id/linearLayoutLevelFour"
        style="@style/fill_wrap"
        android:orientation="vertical"
        android:visibility="gone" >
    </LinearLayout>

</LinearLayout><!-- From: file:/C:/Users/lenovo/Documents/Handheld%20Ordering/Food/app/src/main/res/layout/order_list_item_2.xml -->